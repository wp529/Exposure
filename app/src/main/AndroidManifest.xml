<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wp.exposure">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ListActivity" />
        <activity android:name=".GridActivity" />
        <activity android:name=".StaggerActivity" />
        <activity android:name=".VpFragmentActivity" />
        <activity android:name=".HorizontalRvActivity" />
        <activity android:name=".RvInScrollActivity" />
        <activity android:name=".ExposureInChildActivity" />
        <activity android:name=".CollectViewGroupActivity" />
        <activity android:name=".RvInRvActivity" />
    </application>

</manifest>